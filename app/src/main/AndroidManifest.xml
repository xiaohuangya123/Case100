<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.xhj">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch2019"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launch2019"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".case01_pic_cache.activity.PicDetailActivity"
            android:theme="@style/BitImageActivityTheme" />
        <activity
            android:name=".case01_pic_cache.activity.BigImageActivity"
            android:theme="@style/BitImageActivityTheme"/>
    </application>

</manifest>